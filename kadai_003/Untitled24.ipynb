        "import numpy as np\n",
        "\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "\n",
        "c = np.dot(A, B)\n",
        "\n",
        "print(\"行列の積の結果：\")\n",
        "print(c)\n",
        "\n",
        "c_max = np.max(c)\n",
        "print(\"行列の要素の最大値：\")\n",
        "print(c_max)"
      ],
